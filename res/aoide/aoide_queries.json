    {
        "label": "Library",
        "items": [
            {
                "label": "Recently Added",
                "notes": "Tracks that have recently been added to the collection.",
                "query": {
                    "sort": "-collectedAt"
                }
            },
            {
                "label": "Recently Modified",
                "notes": "Tracks that have recently been created or updated.",
                "query": {
                    "sort": "-updatedAt"
                }
            },
            {
                "label": "Compilations",
                "notes": "Tracks from albums of \"various…artists\".",
                "query": {
                    "filter": {
                        "actorPhrase": {
                            "scope": "album",
                            "roles": [
                                0
                            ],
                            "nameTerms": [
                                "various",
                                "artists"
                            ]
                        }
                    }
                }
            },
            {
                "label": "Missing album artist or title",
                "notes": "Tracks with an album title but no album artist or vice versa.",
                "query": {
                    "sort": "+trackNumber",
                    "filter": {
                        "any": [
                            {
                                "all": [
                                    {
                                        "not": {
                                            "titlePhrase": {
                                                "scope": "album",
                                                "nameTerms": []
                                            }
                                        }
                                    },
                                    {
                                        "actorPhrase": {
                                            "scope": "album",
                                            "roles": [
                                                0
                                            ],
                                            "nameTerms": []
                                        }
                                    }
                                ]
                            },
                            {
                                "all": [
                                    {
                                        "not": {
                                            "actorPhrase": {
                                                "scope": "album",
                                                "roles": [
                                                    0
                                                ],
                                                "nameTerms": []
                                            }
                                        }
                                    },
                                    {
                                        "titlePhrase": {
                                            "scope": "album",
                                            "nameTerms": []
                                        }
                                    }
                                ]
                            }
                        ]
                    }
                }
            },
            {
                "label": "Missing track number",
                "notes": "Album tracks with missing track number.",
                "query": {
                    "filter": {
                        "all": [
                            {
                                "not": {
                                    "actorPhrase": {
                                        "scope": "album",
                                        "roles": [
                                            0
                                        ],
                                        "nameTerms": []
                                    }
                                }
                            },
                            {
                                "not": {
                                    "numeric": [
                                        "trackNumber",
                                        {
                                            "gt": 0
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                }
            },
            {
                "label": "Invalid MusicBrainz IDs",
                "query": {
                    "filter": {
                        "any": [
                            {
                                "tag": {
                                    "facets": {
                                        "anyOf": [
                                            "mbid-trk"
                                        ]
                                    },
                                    "label": {
                                        "equals": "00000000-0000-0000-0000-000000000000"
                                    }
                                }
                            },
                            {
                                "tag": {
                                    "facets": {
                                        "anyOf": [
                                            "mbid-rec"
                                        ]
                                    },
                                    "label": {
                                        "equals": "00000000-0000-0000-0000-000000000000"
                                    }
                                }
                            },
                            {
                                "tag": {
                                    "facets": {
                                        "anyOf": [
                                            "mbid-rel"
                                        ]
                                    },
                                    "label": {
                                        "equals": "00000000-0000-0000-0000-000000000000"
                                    }
                                }
                            },
                            {
                                "tag": {
                                    "facets": {
                                        "anyOf": [
                                            "mbid-rel-grp"
                                        ]
                                    },
                                    "label": {
                                        "equals": "00000000-0000-0000-0000-000000000000"
                                    }
                                }
                            }
                        ]
                    }
                }
            },
            {
                "label": "Unplayable tracks",
                "notes": "Tracks with invalid/unknown duration or sample rate.",
                "query": {
                    "filter": {
                        "any": [
                            {
                                "not": {
                                    "numeric": [
                                        "audioDurationMs",
                                        {
                                            "gt": 0
                                        }
                                    ]
                                }
                            },
                            {
                                "not": {
                                    "numeric": [
                                        "audioSampleRateHz",
                                        {
                                            "gt": 0
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                }
            },
            {
                "label": "Tracked sources",
                "notes": "Tracks with sources that have been imported by the Media Tracker.",
                "query": {
                    "filter": {
                        "condition": "sourceTracked"
                    },
                    "sort": "-updatedAt"
                }
            },
            {
                "label": "Untracked sources",
                "notes": "Tracks with sources that are unknown to the Media Tracker.",
                "query": {
                    "filter": {
                        "condition": "sourceUntracked"
                    },
                    "sort": "-updatedAt"
                }
            }
        ]
    }
]
